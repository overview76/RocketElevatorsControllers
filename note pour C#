cree un OBJ
public class Voiture
{
    public int Vitesse { get; set; }

    public Voiture()
    {
        Vitesse = 5;
    }

    public void Rouler()
    {
        Console.WriteLine("Je roule à " + Vitesse + " km/h");
    }
}